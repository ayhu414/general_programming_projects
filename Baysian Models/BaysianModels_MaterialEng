   "source": [
    "Cp_hyper_prior = stats.uniform(0,10)\n",
    "H_hyper_prior = stats.uniform(0,10)\n",
    "\n",
    "dataset_priors = [Cp_hyper_prior,H_hyper_prior]\n",
    "priors.extend(dataset_priors)\n",
    "print(len(priors))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "id": "060a8c9e-83cc-4d5d-b21d-9500ff5ccebc",
   "metadata": {},
